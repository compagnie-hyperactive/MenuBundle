{
    "name": "lch/menu-bundle",
    "type": "symfony-bundle",
    "description": "Gives a simple Wordpress like menu management system for Symfony",
    "keywords": ["menu", "management", "symfony"],
    "homepage": "https://github.com/compagnie-hyperactive/MenuBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas BONNIOT",
            "email": "nicolas@devgiants.fr"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^4.2",
        "knplabs/doctrine-behaviors": "^2.0",
        "lch/components-bundle": "^1.2",
        "twig/extensions": "^1.5",
        "symfony/translation": "4.4.*"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3",
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "conflict": {
        "symfony/doctrine-bridge": "<2.7"
    },
    "autoload": {
        "psr-4": {
            "Lch\\MenuBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    }
}
